                       {% endif %} 
                    {% endfor %}
                </div>
            </div>
           <div class="cheatGroupSubHolder">
                <a data-cheatgroup="misccheats">
                    <span class="cheatGroupImage">
                    {{ svg.icon('misccheats-solid') }}
                </span>
                    <h5 class="cheatGroupTitle">Miscellaneous</h5>
                </a>
                 <div class="cheatTable">
                    {% for item in gta3 %} 
                       {% if item.type == 'misc' %}
                           <ul>
                               <li>{{ item.result }}</li>
                               <li>{{ item.cheat }}</li> 
                           </ul>
                       {% endif %} 
                    {% endfor %}
                </div>
            </div>
           <div class="cheatGroupSubHolder">
                <a data-cheatall>
                    <span class="cheatGroupImage">
                    {{ svg.icon('allcheats-solid') }}
                </span>
                    <h5 class="cheatGroupTitle">All Cheats</h5>
                </a>
            </div>
    </section>
</article>