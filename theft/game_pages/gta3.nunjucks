                               <li>{{ item.result }}</li>
                               <li>{{ item.cheat }}</li> 
                           </ul>
                       {% endif %} 
                    {% endfor %}
                </div>
            </div>
           <div class="cheatGroupSubHolder">
                <a data-cheatgroup="gameplay">
                    <span class="cheatGroupImage">
                    {{ svg.icon('gameplay-solid') }}
                </span>
                    <h5 class="cheatGroupTitle">Appeal &amp; Gameplay</h5>
                </a>
                 <div class="cheatTable">
                    {% for item in gta3 %} 
